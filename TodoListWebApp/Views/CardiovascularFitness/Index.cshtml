@model IEnumerable<TodoListWebApp.Models.CardiovascularFitness>

@{
    ViewBag.Title = "Index";
}

<h2>Cardiovascular Fitness</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HalfMileTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pacer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MileTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StepTestSteps)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StepTestHeartRate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Logged)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HalfMileTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pacer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MileTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StepTestSteps)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StepTestHeartRate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Logged)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
